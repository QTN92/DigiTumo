<View controllerName="DigiTumo.controller.Dashboard" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout" height="100%">
	<Page title="Dashboard" showNavButton="true" navButtonPress="onBack">
		<l:Grid>
			<!--Datenblatt nicht als Tile sondern Scrollpanel?-->
			<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Gesundheitsscore" press="press">
				<tileContent>
					<TileContent>
						<content>
							<NumericContent icon="sap-icon://stethoscope" value="8" valueColor="Good"/>
						</content>
					</TileContent>
				</tileContent>
			</GenericTile>
		</l:Grid>
	</Page>
</View>